<!-- LOADING BACKGROUND -->
<div id="loading_back"
     class="loading_back">
  <div id="loadinggif"
       style="
          position: absolute;
          z-index: 100;
          top: 50%;
          left: 50%;
          margin-left: -75px;
          margin-top: -80px;
        "
       class="loader"></div>
</div>
  <!-- PLAYER & AJAX REQUEST -->
  <table class="table_center"
         style="color: white; text-shadow: 2px 2px 4px #000000">
    <tr>
      <td colspan="4">
        <div id="player"></div>
      </td>
    </tr>
    <tr>
      <td style="width: 125px">FROM</td>
      <td style="width: 225px">
        <input id="f_fromTime"
               type="number"
               class="textinput"
               style="width: 175px; padding: 2px"
               value="3"
               step="0.01"
               min="0"
               max="100" />
      </td>
      <td style="width: 100px">
        <button id="f_from-set"
                class="button-offset"
                style="width: 75px; text-indent: -14px">
          SET
        </button>
      </td>
      <td style="width: 100px">
        <button id="f_from-goto"
                class="button-offset"
                style="width: 135px">
          GO TO
        </button>
      </td>
    </tr>
    <tr>
      <td style="width: 125px">TO</td>
      <td style="width: 225px">
        <input id="f_toTime"
               type="number"
               class="textinput"
               style="width: 175px; padding: 2px"
               value="18"
               step="0.01"
               min="0"
               max="100" />
      </td>
      <td style="width: 100px">
        <button id="f_to-set"
                class="button-offset"
                style="width: 75px; text-indent: -14px">
          SET
        </button>
      </td>
      <td style="width: 100px">
        <button id="f_to-goto"
                class="button-offset"
                style="width: 135px">
          GO TO
        </button>
      </td>
    </tr>
    <tr>
      <td></td>
      <td colspan="2">
        <button id="f_submit"
                type="submit"
                class="button-offset"
                style="width: 200px">
          DOWNLOAD
        </button>
      </td>
      <td></td>
    </tr>
  </table>